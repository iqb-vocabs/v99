{
  "name": "@iqb/csv2ttl",
  "version": "0.9.0",
  "author": "IQB - Institut zur Qualit√§tsentwicklung im Bildungswesen",
  "license": "MIT",
  "description": "Generate ttl files for publishing metadata vocabularies from csv",
  "scripts": {
    "add_ids": "ts-node src/addidcsv.ts sample_data/deutsch",
    "add_ids_en": "ts-node src/addidcsv.ts sample_data/deutsch-englisch",
    "add_ids_ch": "ts-node src/addidcsv.ts sample_data/sprache-schweiz",
    "add_ids_ca": "ts-node src/addidcsv.ts sample_data/notation-charakter",
    "generate_ttl": "ts-node src/csv2ttl.ts sample_data/deutsch",
    "generate_ttl_en": "ts-node src/csv2ttl.ts sample_data/deutsch-englisch",
    "generate_ttl_ch": "ts-node src/csv2ttl.ts sample_data/sprache-schweiz",
    "generate_ttl_ca": "ts-node src/csv2ttl.ts sample_data/notation-charakter",
    "prepare_publish": "rm -rf dist_npm/* && tsc && cp ./src/csv2ttl_config.schema.json ./dist_npm",
    "npm_publish": "npm publish --access public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/iqb-vocabs/csv2ttl.git"
  },
  "bugs": {
    "url": "https://github.com/iqb-vocabs/csv2ttl/issues"
  },
  "homepage": "https://github.com/iqb-vocabs/csv2ttl#readme",
  "eslintConfig": {
    "extends": "@iqb/eslint-config",
    "parserOptions": {
      "project": "./tsconfig.json"
    },
    "rules": {
      "no-console": "off",
      "class-methods-use-this": [
        "error",
        {
          "exceptMethods": [
            "getElementComponent",
            "getNewOptionLabel",
            "transform"
          ]
        }
      ],
      "import/export": 0
    }
  },
  "dependencies": {
    "@json2csv/plainjs": "^7.0.3",
    "ajv": "^8.12.0",
    "csv-parse": "^5.5.0",
    "randexp": "^0.5.3"
  },
  "devDependencies": {
    "@iqb/eslint-config": "^2.1.1",
    "@types/node": "^20.4.4",
    "eslint": "^8.54.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2"
  },
  "bin": {
    "csv2ttl": "./dist_npm/csv2ttl.js",
    "addidcsv": "./dist_npm/addidcsv.js"
  }
}
